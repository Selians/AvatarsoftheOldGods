        <race>S_GodPrimordial</race>
        <chemicalAddictionChance>0</chemicalAddictionChance>
        <apparelTags>
			<li>Outlander</li>
        </apparelTags>
        <weaponTags>
			<li>MedievalMeleeAdvanced</li>
		</weaponTags>
        <apparelAllowHeadgearChance>0</apparelAllowHeadgearChance>
        <apparelMoney>1500~6000</apparelMoney>
    </PawnKindDef>

