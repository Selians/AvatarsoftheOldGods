<Defs>

    <PawnKindDef Name="GodPawnKindBase" Abstract="True">
        <apparelIgnoreSeasons>true</apparelIgnoreSeasons>
        <forceNormalGearQuality>true</forceNormalGearQuality>
        <combatPower>30</combatPower>
        <defaultFactionType>AOGPlayerFaction</defaultFactionType>
        <initialWillRange>4~5</initialWillRange>
		<initialResistanceRange>45~54</initialResistanceRange>
        <backstoryCryptosleepCommonality>1</backstoryCryptosleepCommonality>
        <maxGenerationAge>2000</maxGenerationAge>
        <backstoryFiltersOverride>
            <li>
                <categories>
                    <li>DivineBackstory</li>
                </categories>
            </li>
        </backstoryFiltersOverride>
	</PawnKindDef>

    <PawnKindDef ParentName="GodPawnKindBase">
        <defName>PrimordialPK</defName>
